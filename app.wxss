/**app.wxss**/
@import 'weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.Card{
  border: 1px solid #f0f0f0;
  padding: 10rpx;
  border-radius: 10rpx;
  box-shadow: 5rpx 5rpx 20rpx rgba(0, 0, 0, .08);
}